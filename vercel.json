{
  "version": 2,
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/order.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.py",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/order",
      "dest": "/api/order.py",
      "methods": ["POST"]
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
